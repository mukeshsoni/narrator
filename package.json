{
  "name": "puppeteer-puppeteer",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "scripts": {
    "build:recorder": "rollup -c rollup.config.js src/recorder/recorder.js --file ./build/recorder.js --format umd --name PuppeteerRecorder",
    "start:css": "postcss src/tailwind.css -o src/tailwind_generated.css",
    "watch:css": "postcss src/tailwind.css -o src/tailwind_generated.css",
    "build:css:prod": "postcss ./src/tailwind.css -o ./src/tailwind_generated.css --env production",
    "start": "npm-run-all start:css start:app",
    "start:app": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "autoprefixer": "^9.8.2",
    "electron": "^9.0.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.1",
    "rollup": "^2.18.1",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "@medv/finder": "^2.0.0",
    "puppeteer": "^4.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-onclickoutside": "^6.9.0",
    "ua-parser-js": "^0.7.21"
  }
}
